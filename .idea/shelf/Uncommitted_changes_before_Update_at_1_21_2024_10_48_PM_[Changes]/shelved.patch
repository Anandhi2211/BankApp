diff --git a/src/main/resources/mappers/Customer.xml b/src/main/resources/mappers/Customer.xml
new file mode 100644
--- /dev/null	(date 1705824147006)
+++ b/src/main/resources/mappers/Customer.xml	(date 1705824147006)
@@ -0,0 +1,32 @@
+<?xml version="1.0" encoding="UTF-8" ?>
+<!DOCTYPE mapper
+        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
+        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
+<mapper>
+    <select id="selectAllCustomers" resultType="com.example.model.Customer">
+        SELECT * FROM Customers;
+    </select>
+
+    <!-- Select a customer by SSN -->
+    <select id="selectCustomerBySSN" resultType="com.example.model.Customer" parameterType="java.lang.Long">
+        SELECT * FROM Customers WHERE SSN = #{SSN};
+    </select>
+
+    <!-- Insert a new customer -->
+    <insert id="insertCustomer" parameterType="com.example.model.Customer">
+        INSERT INTO Customers (SSN, first_name, last_name, email, phone_number)
+        VALUES (#{SSN}, #{firstName}, #{lastName}, #{email}, #{phoneNumber});
+    </insert>
+
+    <!-- Update a customer -->
+    <update id="updateCustomer" parameterType="com.example.model.Customer">
+        UPDATE Customers
+        SET first_name = #{firstName}, last_name = #{lastName}, email = #{email}, phone_number = #{phoneNumber}
+        WHERE SSN = #{SSN};
+    </update>
+
+    <!-- Delete a customer by SSN -->
+    <delete id="deleteCustomer" parameterType="java.lang.Long">
+        DELETE FROM Customers WHERE SSN = #{SSN};
+    </delete>
+</mapper>
